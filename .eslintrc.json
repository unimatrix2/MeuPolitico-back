{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
    "class-methods-use-this": "off",
        "no-restricted-syntax": "off",
        "indent": [4, "tab"],
        "no-tabs": 0,
        "no-mixed-spaces-and-tabs": "warn"
    }
}
